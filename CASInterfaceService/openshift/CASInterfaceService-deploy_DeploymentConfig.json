{
    "kind": "List",
    "apiVersion": "v1",
    "metadata": {},
    "items": [
        {
            "apiVersion": "apps.openshift.io/v1",
            "kind": "DeploymentConfig",
            "metadata": {
                "annotations": {
                    "description": "Defines how to deploy cas-interface-service"
                },
                "labels": {
                    "template": "cas-interface-service-deployment"
                },
                "name": "cas-interface-service"
            },
            "spec": {
                "replicas": 1,
                "selector": {
                    "name": "cas-interface-service"
                },
                "strategy": {
                    "type": "Rolling"
                },
                "template": {
                    "metadata": {
                        "labels": {
                            "name": "cas-interface-service"
                        },
                        "name": "cas-interface-service"
                    },
                    "spec": {
                        "containers": [
                            {
                                "env": [
                                    {
                                        "name": "ASPNETCORE_ENVIRONMENT",
                                        "value": "Production"
                                    }
                                ],
                                "image": "",
                                "liveinessProbe": {
                                    "failureThreshold": 3,
                                    "httpGet": {
                                        "path": "/hc",
                                        "port": 8080,
                                        "scheme": "HTTP"
                                    },
                                    "initialDelaySeconds": 10,
                                    "periodSeconds": 10,
                                    "successThreshold": 1,
                                    "timeoutSeconds": 1
                                },
                                "name": "cas-interface-service",
                                "ports": [
                                    {
                                        "containerPort": 8080,
                                        "protocol": "TCP"
                                    }
                                ],
                                "readinessProbe": {
                                    "failureThreshold": 3,
                                    "httpGet": {
                                        "path": "/hc",
                                        "port": 8080,
                                        "scheme": "HTTP"
                                    },
                                    "initialDelaySeconds": 10,
                                    "periodSeconds": 10,
                                    "successThreshold": 1,
                                    "timeoutSeconds": 1
                                },
                                "resources": {
                                    "limits": {
                                        "cpu": "1",
                                        "memory": "4Gi"
                                    },
                                    "requests": {
                                        "cpu": "100m",
                                        "memory": "2Gi"
                                    }
                                }
                            }
                        ]
                    }
                },
                "triggers": [
                    {
                        "type": "ConfigChange"
                    },
                    {
                        "imageChangeParams": {
                            "automatic": true,
                            "containerNames": [
                                "cas-interface-service"
                            ],
                            "from": {
                                "kind": "ImageStreamTag",
                                "name": "cas-interface-service:dev",
                                "namespace": "pssg-cscp-vsd-tools"
                            }
                        },
                        "type": "ImageChange"
                    }
                ]
            }
        },
        {
            "apiVersion": "v1",
            "kind": "Service",
            "metadata": {
                "annotations": {
                    "description": "Exposes and load balances the application pods."
                },
                "labels": {
                    "template": "cas-interface-service-deployment"
                },
                "name": "cas-interface-service"
            },
            "spec": {
                "ports": [
                    {
                        "name": "8080-tcp",
                        "port": 8080,
                        "protocol": "TCP",
                        "targetPort": 8080
                    }
                ],
                "selector": {
                    "name": "cas-interface-service"
                },
                "sessionAffinity": "None",
                "type": "ClusterIP"
            }
        },
        {
            "apiVersion": "route.openshift.io/v1",
            "kind": "Route",
            "metadata": {
                "labels": {
                    "app": "cas-interface-service"
                },
                "name": "cas-interface-service"
            },
            "spec": {
                "host": "cas-interface-service-dev.pathfinder.gov.bc.ca",
                "port": {
                    "targetPort": "8080-tcp"
                },
                "tls": {
                    "insecureEdgeTerminationPolicy": "Redirect",
                    "termination": "edge"
                },
                "to": {
                    "kind": "Service",
                    "name": "cas-interface-service",
                    "weight": 100
                }
            }
        },
        {
            "apiVersion": "route.openshift.io/v1",
            "kind": "Route",
            "metadata": {
                "labels": {
                    "app": "cas-interface-service-siteminder-route"
                },
                "name": "cas-interface-service-siteminder-route"
            },
            "spec": {
                "host": "cas-interface-service-dev.pathfinder.bcgov",
                "port": {
                    "targetPort": "8080-tcp"
                },
                "to": {
                    "kind": "Service",
                    "name": "cas-interface-service",
                    "weight": 100
                }
            }
        },
        {
            "apiVersion": "autoscaling/v1",
            "kind": "HorizontalPodAutoscaler",
            "metadata": {
                "labels": {
                    "app": "cas-interface-service"
                },
                "name": "cas-interface-service"
            },
            "spec": {
                "maxReplicas": 6,
                "minReplicas": 2,
                "scaleTargetRef": {
                    "kind": "DeploymentConfig",
                    "name": "cas-interface-service"
                }
            }
        },
        {
            "apiVersion": "v1",
            "kind": "PersistentVolumeClaim",
            "metadata": {
                "name": "KEY_RING_STORAGE_NAME"
            },
            "spec": {
                "accessModes": [
                    "ReadWriteOnce"
                ],
                "resources": {
                    "requests": {
                        "storage": "5Gi"
                    }
                }
            }
        }
    ]
}
